{
  "apps": {
    "http": {
      "servers": {
        "srv0": {
          "listen": [
            ":443"
          ],
          "routes": [
            {
              "handle": [
                {
                  "handler": "subroute",
                  "routes": [
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "headers": {
                            "response": {
                              "set": {
                                "Strict-Transport-Security": [
                                  "max-age=31536000;"
                                ]
                              }
                            }
                          },
                          "upstreams": [
                            {
                              "dial": "app:3005"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ],
              "match": [
                {
                  "host": [
                    "paranjay-test.logicwind.co"
                  ]
                }
              ],
              "terminal": true
            }
          ]
        }
      }
    }
  }
}
